        "esbuild": "^0.20.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "globals": "^15.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "typescript-eslint": "^7.8.0",
        "vitest": "^1.3.1"
    }
