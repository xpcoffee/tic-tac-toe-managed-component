{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "es2022",
        "module": "commonjs",
        "moduleResolution": "node",
        "types": [
            "vitest/globals"
        ],
        "resolveJsonModule": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}